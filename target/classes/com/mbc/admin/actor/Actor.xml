<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mbc.admin.actor.ActorService">
	<insert id="actorinsert">
		insert into actor values(actor_seq.nextval,#{param1},#{param2},#{param3},#{param4},#{param5})
	</insert>
	<select id="actorout" resultType="com.mbc.admin.actor.ActorDTO">
		select * from actor
	</select>
	<select id="update" resultType="com.mbc.admin.actor.ActorDTO">
		select * from actor where actornum = #{param1}
	</select>
	<update id="update2">
		update actor set actor=#{param2}, actorimage=#{param3},actortv1=#{param4},actortv2=#{param5},actortv3=#{param6} where actornum=#{param1}
	</update>
</mapper>